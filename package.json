{
  "name": "qa-web-automation",
  "version": "1.0.0",
  "description": "web automation testing using cypress",
  "main": "index.js",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^18.0.5",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.3",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^11.0.0",
    "@cucumber/cucumber": "^9.1.0",
    "@cucumber/cucumber-expressions": "^16.1.2",
    "@cucumber/gherkin": "^26.2.0",
    "@cucumber/html-formatter": "^20.3.0",
    "@cucumber/message-streams": "^4.0.1",
    "@cucumber/messages": "^22.0.0",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@cucumber/tag-expressions": "^5.0.1",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@cypress/webpack-preprocessor": "^5.12.2",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@percy/cypress": "^3.1.2",
    "@types/cucumber": "^6.0.1",
    "@types/cypress": "^1.1.3",
    "@types/eslint": "^7.2.3",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.10",
    "@types/prettier": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-preset-es2015": "^6.24.1",
    "base64-js": "^1.5.1",
    "chalk": "^4.1.2",
    "cli-table": "^0.3.11",
    "common-ancestor-path": "^1.0.1",
    "cosmiconfig": "^8.1.3",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^5.5.0",
    "cypress": "^13.2.0",
    "cypress-cucumber-attach-screenshots-to-failed-steps": "^1.0.0",
    "cypress-file-upload": "^5.0.2",
    "cypress-fill-command": "^1.0.2",
    "cypress-mailosaur": "^2.1.3",
    "cypress-multi-reporters": "^1.4.0",
    "cypress-xpath": "^2.0.1",
    "debug": "^4.3.4",
    "error-stack-parser": "^2.1.4",
    "esbuild": "^0.19.0",
    "eslint": "^7.10.0",
    "faker": "^5.5.3",
    "glob": "^10.2.2",
    "husky": "^4.2.5",
    "is-path-inside": "^3.0.3",
    "lint-staged": "^10.4.0",
    "mailosaur": "^7.0.3",
    "mocha": "^10.2.0",
    "mochawesome": "^7.0.1",
    "mochawesome-merge": "^4.1.0",
    "mochawesome-report-generator": "^6.0.1",
    "multiple-cucumber-html-reporter": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "seedrandom": "^3.0.5",
    "source-map": "^0.7.4",
    "split": "^1.0.1",
    "ts-loader": "^8.0.3",
    "tsify": "^5.0.4",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "cy:verify": "cypress verify",
    "cy:version": "cypress version",
    "precy:run": "rm -rf cypress/screenshots cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochawesome-report",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "report": "run-s report:*",
    "lint:ts": "eslint \"cypress/**/*.ts\"",
    "prettier": "prettier --write \"cypress/**/*.{ts,json}\"",
    "report:merge": "mochawesome-merge cypress/reports/mochawesome-report/*.json > cypress/reports/mochawesome-report/report.json",
    "report:generate": "marge cypress/reports/mochawesome-report/report.json -f report -o cypress/reports/mochawesome-report",
    "delete:existingJsonReports": "rm -rf cypress/reports/cucumber-json/*.json",
    "delete:reportFolder": "rm -rf cypress/reports/",
    "cucumber:report": "node cucumber-html-report.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "cypress/**/*.{ts,json}": "prettier --write"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/**/*.{js,ts}",
    "commonPath": "cypress/e2e/**",
    "filterSpecs": true,
    "omitFiltered": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "blob-util": "^2.0.2",
    "cheerio": "^1.0.0-rc.12",
    "cypress-commands": "^2.0.1",
    "cypress-wait-until": "^1.7.2",
    "date-fns": "^2.28.0"
  }
}
